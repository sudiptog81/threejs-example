{
  "name": "threejs-example",
  "version": "0.0.1",
  "description": "ThreeJS Example",
  "main": "src/index.js",
  "scripts": {
    "start": "serve",
    "dev": "cp src/index.html dist/index.html && cp -r src/assets dist/assets && webpack-dev-server --open --mode development",
    "build": "webpack --mode production && cp -r src/assets dist/assets && cp src/index.html dist/index.html",
    "now-build": "npm run build",
    "lint": "eslint --fix src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.ghosh.pro/sudipto/threejs-example.git"
  },
  "keywords": [
    "threejs",
    "3d",
    "model",
    "rendering"
  ],
  "author": "Sudipto Ghosh",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "browserlist": "^1.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "serve": "^11.0.0"
  },
  "dependencies": {
    "three": "^0.104.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/dist/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  },
  "browserslist": "last 2 versions"
}